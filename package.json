{
  "name": "sidruxell-web",
  "description": "Gatsby Sid Ruxell website",
  "version": "1.0.0",
  "author": "Daudr <michele@daudr.me>",
  "bugs": {
    "url": "https://github.com/Daudr/sidruxell-web/issues"
  },
  "dependencies": {
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-source-contentful": "^2.3.4",
    "gatsby-source-filesystem": "^2.3.2",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "sharp": "^0.25.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "contentful-import": "^7.7.14",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "gatsby": "^2.21.25",
    "gh-pages": "^2.2.0",
    "inquirer": "^7.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "yargs-parser": "^18.1.3"
  },
  "homepage": "https://sidruxell.com",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daudr/sidruxell-web.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "yarn remove contentful-import chalk inquirer && node ./bin/cleanup.js && rimraf bin contentful"
  }
}
